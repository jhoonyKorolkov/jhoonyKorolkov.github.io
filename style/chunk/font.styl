/* montserrat-300 - cyrillic_cyrillic-ext */
@font-face {
  font-family: 'Montserrat';
  font-style: normal;
  font-weight: 300;
  src: url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.eot'); /* IE9 Compat Modes */
  src: local('Montserrat Light'), local('Montserrat-Light'),
       url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.woff2') format('woff2'), /* Super Modern Browsers */
       url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.woff') format('woff'), /* Modern Browsers */
       url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../font/montserrat-v12-cyrillic_cyrillic-ext-300.svg#Montserrat') format('svg'); /* Legacy iOS */
}
